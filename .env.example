# Environment Variable: VITE_SESSION_BUFFER_MINUTES

# Session buffer time in minutes
# Session remains "live" for this duration after the scheduled end time
# 
# Use cases:
# - Production: 10 minutes (default)
# - QA/Testing: 1 minute for faster testing
# - Extended grace period: 15 minutes for large classes
#
# Default: 10 minutes (if not specified)

VITE_SESSION_BUFFER_MINUTES=10
